@model IEnumerable<DataHUBWebApplication.Models.Course>

@{
    ViewData["Title"] = "Pricing";
}

<h1>Pricing</h1>

<table class="table">
    <thead>
        <tr>
            <th>Course</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var course in Model)
        {
            <tr>
                <td>@course.CourseName</td>
                <td>@course.Price.ToString("C")</td>
            </tr>
        }
    </tbody>
</table>